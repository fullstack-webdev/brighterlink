    body {
      background-color: #bcc6cf;
    }

    .main-container {
      max-width: 1920px;
      min-height: 100%;
    }

    .main-container .error-background {
      position: absolute;
      width: 100%;
      height: 100%;
      overflow: hidden;
    }

    .main-container .error-background ul {
      padding: 0;
      margin: 0;
    }

    .main-container .error-background ul li img {
      width: 100%;
      height: 100%;
      background-size: cover;
    }

    .main-container .container-bar {
      position: relative;
      width: 100%;
      height: 9px;
      background-color: #333c4e;
    }

    .main-container .l-logo {
      position: relative;
      margin: 0 auto;
      width: 112px;
      height: 70px;
      background: url(/assets/img/login-logo.png);
    }

    .main-container .internal-error-title {
      position: relative;
      margin-top: 60px;
      text-align: center;
      font-size: 50px;
      font-family: "HelveticaNeueW02-75Bold", Helvetica, Arial;
      font-weight: 400;
      color: #5a6477;
    }

    .main-container .error-description {
      position: relative;
      text-align: center;
      margin-top: 55px;
      font-family: "HelveticaNeueW02-75Bold", Helvetica, Arial;
      font-size: 20px;
      font-weight: 800;
      color: #5a6477;
    }

    .main-container .error-service {
      position: relative;
      margin-top: 20px;
      text-align: center;
      font-size: 16px;
      font-family: "HelveticaNeue-55 Roman", Helvetica, Roman;
      font-weight: 400;
      color: #8691a5;
    }

    .main-container .guidance {
      position: relative;
      margin-top: 30px;
      text-align: center;
    }

    .guidance-link {
      padding: 0;
    }

    .main-container .guidance ul li {
      height: 25px;
      list-style: none;
    }

    .main-container .guidance ul li a {
      color: #007ebc;
      font-size: 14px;
      font-family: "HelveticaNeue-75Medi", Helvetica, Bold;
      font-weight: 800;
      text-decoration: none;
    }

    .main-container .report-error {
      position: relative;
      font-family: "HelveticaNeueW02-65Medi", Helvetica, Roman;
      font-size: 14px;
      margin-top: 460px;
      text-align: center;
    }

    .main-container .report-error span {
      color: #616a7b;
    }

    .main-container .report-error a {
      color: #ed8e66;
      text-decoration: underline;
    }

    .sky {
      display: block;
    }

    .cloud {
      display: block;
    }

    .city {
      display: block;
    }

    .train {
      display: block;
    }

